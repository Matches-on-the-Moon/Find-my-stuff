<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.motm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:name="com.motm.application.FMSApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.motm.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.motm.activities.LoginActivity"
            android:label="@string/loginActivityLabel" />
        <activity
            android:name="com.motm.activities.RegisterAccountActivity"
            android:label="@string/registerActivityLabel" />
        <activity
            android:name="com.motm.activities.FindItemActivity"
            android:label="@string/findItemActivityLabel" />
        <activity
            android:name="com.motm.activities.AddItemActivity"
            android:label="@string/addItemActivityLabel" />
        <activity
            android:name="com.motm.activities.ViewItemActivity"
            android:label="View an account" />
        <activity
            android:name="com.motm.activities.FindAccountActivity"
            android:label="Find an account" />
        <activity
            android:name="com.motm.activities.ViewAccountActivity"
            android:label="View an account" />
        <activity
            android:name="com.motm.activities.EditAccountActivity"
            android:label="Edit an account." />
        <activity
            android:name="com.motm.activities.EditItemActivity"
            android:label="Edit an item." />
        <activity android:name="com.motm.activities.RegisterItemActivity" android:label="Add an item."></activity>
    </application>

</manifest>